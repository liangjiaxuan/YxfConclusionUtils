<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rongwei.yxfconclusionutils">

    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".aui.activity.RecyclerviewHelperActivity" />
        <activity android:name=".aui.activity.rv.AnimationUseActivity" />
        <activity android:name=".aui.activity.rv.MultipleItemUseActivity" />
        <activity android:name=".aui.activity.rv.HeaderAndFooterUseActivity" />
        <activity android:name=".aui.activity.rv.PullToRefreshUseActivity" />
        <activity android:name=".aui.activity.rv.EmptyViewUseActivity" />
        <activity android:name=".aui.activity.SmartTablelayoutActivity" />
        <activity android:name=".aui.activity.PopWindowActivity" />
        <activity android:name=".aui.activity.DateAndTimeUtilsActivity" />
        <activity android:name=".aui.activity.NetStateActivity" />
        <activity android:name=".aui.activity.CustomDialogActivity" />
        <activity android:name=".aui.activity.NotificationUtilsActivity" />
        <activity android:name=".permission.PermissionsActivity" />
        <activity android:name=".aui.activity.MyPermissionActivity"></activity>
    </application>

</manifest>